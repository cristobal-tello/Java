<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	
	<context:annotation-config/>	<!-- This app will use annotations -->
	
	<bean id="unique_id_dairy" class="autoWiringProperties.Dairy" />
	<bean id="unique_id_fruit" class="autoWiringProperties.Fruit" />
	<bean id="unique_id_grain" class="autoWiringProperties.Grain" />
	<bean id="unique_id_meat" class="autoWiringProperties.Meat" />
	<bean id="unique_id_vegetable" class="autoWiringProperties.Vegetable" />

	<bean id ="unique_id_meal" class="autoWiringProperties.Meal" />
	
</beans>
