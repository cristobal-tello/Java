<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="unique_id_fruit" class="simpleSpringApp.Fruit" />

	<bean id="unique_id_fruit_orange" class="simpleSpringApp.Fruit">
		<constructor-arg index="0" value="Orange" />
	</bean>
	
	<bean id="unique_id_vegetable" class="simpleSpringApp.Vegetable" />
	

</beans>
